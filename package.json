{
  "name": "tf2-rtp-calculator",
  "version": "1.1.0",
  "description": "An RTP calculator for Team Fortress 2 crate unbox sessions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "help": "node dist/index.js --help",
    "install-data": "node dist/index.js install --verbose",
    "update-data": "node dist/index.js install --update --verbose"
  },
  "bin": {
    "tf2-rtp-calculator": "./dist/index.js"
  },
  "keywords": [
    "Team Fortress 2", "TF2",
    "Steam", "RTP",
    "Calculator", "Calculation"
  ],
