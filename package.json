  "license": "MIT",
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/stream-json": "^1.7.8",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "stream-json": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.21"
  }
}
